#Requires AutoHotkey v2.0

; Media
NumpadDot::Media_Play_Pause             ; play/pause
NumpadSub::Send "{ Volume_Up 5 }"       ; volume up
NumpadAdd::Send "{ Volume_Down 5 }"     ; volume down

; Voicemeeter
Numpad4::F15        ; game down
Numpad7::F18        ; game up
Numpad5::F16        ; desktop down
Numpad8::F19        ; desktop up
Numpad6::F17        ; chat down
Numpad9::F20        ; chat up

; Discord
NumpadEnter::F14    ; push to mute
Numpad0::F13        ; Disconnect from Voice
Numpad3::F21        ; toggle mute
